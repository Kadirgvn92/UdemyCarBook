@using UdemyCarBook.DTO.TagCloudDTOs

@model List<ResultTagCloudDTO>

<div class="sidebar-box ftco-animate">
    <h3>Etiketler</h3>
    <div class="tagcloud">
        @foreach(var item in Model)
        {
            <a href="#" class="tag-cloud-link">@item.Title</a>
        }
       
    </div>
</div>
